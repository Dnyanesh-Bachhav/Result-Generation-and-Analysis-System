<hibernate-mapping>
  <class name="beans.Admin_info" table="admin_info">
  <id name="id" type="java.lang.Integer" column="id">
            <generator class="identity"/>
  </id>
  <property name="currentTerm" column="currentTerm"/>
  <property name="currentExaminationYear" column="currentExaminationYear"/>
  <property name="isResultDeclared" column="isResultDeclared"/>
  <property name="lastExaminationYear" column="lastExaminationYear"/>
  </class>
</hibernate-mapping>
